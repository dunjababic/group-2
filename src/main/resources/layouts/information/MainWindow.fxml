<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="gui.controller.information.MainWindowController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="30"/>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="70"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints percentHeight="7" vgrow="SOMETIMES"/>
        <RowConstraints percentHeight="93" vgrow="SOMETIMES"/>
    </rowConstraints>
    <ListView fx:id="cinemaListView" onMouseClicked="#onCinemaListItemClicked" prefHeight="525.0" prefWidth="230.0"
              GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </ListView>
    <AnchorPane fx:id="informationPanel" prefHeight="525.0" prefWidth="100.0" GridPane.columnIndex="1"
                GridPane.rowIndex="1">
        <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </GridPane.margin>
    </AnchorPane>
    <AnchorPane fx:id="toolBar" GridPane.columnSpan="2"/>
</GridPane>
